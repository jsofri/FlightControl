<Window x:Class="PlaneController.IvalidParametersWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="IvalidParameters" WindowStartupLocation="CenterScreen"
        Height="150" Width="250"
        Style="{StaticResource WindowStyle}">
    <Grid>
        <Label Style="{StaticResource LabelTitleStyle}" Content="Invalid IP/ Port entered" HorizontalAlignment="Center" />
        <Button Style="{StaticResource ButtonStyle}" Content="OK" FontSize="20" Margin="64,54,58.2,28.8"
                Click="Button_Click" HorizontalAlignment="Center" Width="121"/>
    </Grid>
</Window>
